<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="root" prefHeight="401.0" prefWidth="796.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.OrderFormController">
   <children>
      <Label alignment="CENTER" layoutY="14.0" prefHeight="34.0" prefWidth="796.0" text="Order Form" textFill="#9a0505">
         <font>
            <Font name="System Bold" size="23.0" />
         </font>
      </Label>
      <Button layoutX="5.0" layoutY="6.0" mnemonicParsing="false" onAction="#btnBackToHome" text="Back To Home">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <TableView fx:id="tblOrder" layoutX="14.0" layoutY="65.0" prefHeight="322.0" prefWidth="766.0">
        <columns>
          <TableColumn fx:id="colOrderI" prefWidth="75.0" text="Order Id" />
          <TableColumn fx:id="colCustomerId" prefWidth="120.0" text="Customer Id" />
            <TableColumn fx:id="colOrderDate" prefWidth="268.0" text="Order Date" />
            <TableColumn fx:id="colOrderTime" prefWidth="105.0" text="Order Time" />
            <TableColumn fx:id="colOrderCost" prefWidth="197.0" text="Total Cost" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
